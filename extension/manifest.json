{
  "manifest_version": 3,
  "name": "Credible.io Text Extractor",
  "version": "1.0",
  "description": "Extracts educational content for credibility analysis",
  "permissions": ["activeTab", "scripting", "notifications"],
  "host_permissions": ["http://127.0.0.1:8000/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Analyze with Credible.io"
  }
}


